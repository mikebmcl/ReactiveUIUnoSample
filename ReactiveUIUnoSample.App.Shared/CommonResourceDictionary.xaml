<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Note that we are including a fallback to the generic styles 'sans-serif' and 'serif' here. This way if there's some problem with the font or it cannot be used, the system will know to fallback to a default font that matches our general intention rather than picking a font without any hint about what might be appropriate. -->
    <x:String x:Key="NotoSans">ms-appx:///Assets/Fonts/NotoSans-Regular.ttf#Noto Sans,sans-serif</x:String>
    <x:String x:Key="NotoSerif">ms-appx:///Assets/Fonts/NotoSerif-Regular.ttf#Noto Serif,serif</x:String>

    <!-- To use these bold fonts you also need to set FontWeight="Bold" -->
    <x:String x:Key="NotoSansBold">ms-appx:///Assets/Fonts/NotoSans-Bold.ttf#Noto Sans,sans-serif</x:String>
    <x:String x:Key="NotoSerifBold">ms-appx:///Assets/Fonts/NotoSerif-Bold.ttf#Noto Serif,serif</x:String>

    <!-- To use this italic font you also need to set FontStyle="Italic" -->
    <x:String x:Key="NotoSansItalic">ms-appx:///Assets/Fonts/NotoSans-Italic.ttf#Noto Sans,sans-serif</x:String>
    <x:String x:Key="NotoSerifItalic">ms-appx:///Assets/Fonts/NotoSerif-Italic.ttf#Noto Serif,serif</x:String>

    <!-- To use these bold fonts you also need to set FontWeight="Bold" and FontStyle="Italic" -->
    <x:String x:Key="NotoSansBoldItalic">ms-appx:///Assets/Fonts/NotoSans-BoldItalic.ttf#Noto Sans,sans-serif</x:String>
    <x:String x:Key="NotoSerifBoldItalic">ms-appx:///Assets/Fonts/NotoSerif-BoldItalic.ttf#Noto Serif,serif</x:String>

    <!-- Defaults -->
    <Style TargetType="ContentControl">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>
    <Style TargetType="UserControl">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>
    <Style TargetType="TextBox" BasedOn="{StaticResource DefaultTextBoxStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>
    <Style TargetType="RadioButton" BasedOn="{StaticResource DefaultRadioButtonStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>
    <Style TargetType="Button" BasedOn="{StaticResource DefaultButtonStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>
    <Style TargetType="ComboBox">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>
    <Style TargetType="ComboBoxItem">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>
    <Style TargetType="ListView">
         <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>
    <Style TargetType="ListViewItem" BasedOn="{StaticResource DefaultListViewItemStyle}">
         <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>
    <Style TargetType="NavigationView">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>
    <Style TargetType="NavigationViewItem">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>
    <Style TargetType="NavigationViewItemHeader">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>

    <Style TargetType="TextBlock" BasedOn="{StaticResource BaseTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>

    <!-- Styles with named keys -->

    <!-- Sans styles -->
    <Style x:Key="SansCaptionTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource CaptionTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>
    <Style x:Key="SansBodyTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource BodyTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>
    <Style x:Key="SansBodyMediumTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource BodyTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
        <Setter Property="FontSize" Value="16" />
    </Style>
    <Style x:Key="SansBodyLargeTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource BodyTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
        <Setter Property="FontSize" Value="20" />
    </Style>
    <Style x:Key="SansSubtitleTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource SubtitleTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>
    <Style x:Key="SansTitleTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource TitleTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>
    <Style x:Key="SansTitleLargeTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource TitleLargeTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSans}" />
    </Style>

    <!-- Sans Bold styles -->
    <Style x:Key="SansBoldCaptionTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource CaptionTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansBold}" />
        <Setter Property="FontWeight" Value="Bold" />
    </Style>
    <Style x:Key="SansBoldBodyTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource BodyTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansBold}" />
        <Setter Property="FontWeight" Value="Bold" />
    </Style>
    <Style x:Key="SansBoldBodyMediumTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource BodyTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansBold}" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="FontSize" Value="16" />
    </Style>
    <Style x:Key="SansBoldBodyLargeTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource BodyTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansBold}" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="FontSize" Value="20" />
    </Style>
    <Style x:Key="SansBoldSubtitleTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource SubtitleTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansBold}" />
        <Setter Property="FontWeight" Value="Bold" />
    </Style>
    <Style x:Key="SansBoldTitleTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource TitleTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansBold}" />
        <Setter Property="FontWeight" Value="Bold" />
    </Style>
    <Style x:Key="SansBoldTitleLargeTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource TitleLargeTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansBold}" />
        <Setter Property="FontWeight" Value="Bold" />
    </Style>

    <!-- Sans Italic styles -->
    <Style x:Key="SansItalicCaptionTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource CaptionTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansItalic}" />
        <Setter Property="FontStyle" Value="Italic" />
    </Style>
    <Style x:Key="SansItalicBodyTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource BodyTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansItalic}" />
        <Setter Property="FontStyle" Value="Italic" />
    </Style>
    <Style x:Key="SansItalicBodyMediumTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource BodyTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansItalic}" />
        <Setter Property="FontStyle" Value="Italic" />
        <Setter Property="FontSize" Value="16" />
    </Style>
    <Style x:Key="SansItalicBodyLargeTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource BodyTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansItalic}" />
        <Setter Property="FontStyle" Value="Italic" />
        <Setter Property="FontSize" Value="20" />
    </Style>
    <Style x:Key="SansItalicSubtitleTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource SubtitleTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansItalic}" />
        <Setter Property="FontStyle" Value="Italic" />
    </Style>
    <Style x:Key="SansItalicTitleTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource TitleTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansItalic}" />
        <Setter Property="FontStyle" Value="Italic" />
    </Style>
    <Style x:Key="SansItalicTitleLargeTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource TitleLargeTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansItalic}" />
        <Setter Property="FontStyle" Value="Italic" />
    </Style>

    <!-- Sans Bold Italic styles -->
    <Style x:Key="SansBoldItalicCaptionTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource CaptionTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansBoldItalic}" />
        <Setter Property="FontStyle" Value="Italic" />
    </Style>
    <Style x:Key="SansBoldItalicBodyTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource BodyTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansBoldItalic}" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="FontStyle" Value="Italic" />
    </Style>
    <Style x:Key="SansBoldItalicBodyMediumTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource BodyTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansBoldItalic}" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="FontStyle" Value="Italic" />
        <Setter Property="FontSize" Value="16" />
    </Style>
    <Style x:Key="SansBoldItalicBodyLargeTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource BodyTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansBoldItalic}" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="FontStyle" Value="Italic" />
        <Setter Property="FontSize" Value="20" />
    </Style>
    <Style x:Key="SansBoldItalicSubtitleTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource SubtitleTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansBoldItalic}" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="FontStyle" Value="Italic" />
    </Style>
    <Style x:Key="SansBoldItalicTitleTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource TitleTextBlockStyle}">
        <Setter Property="FontFamily" Value="{StaticResource NotoSansBoldItalic}" />
        <Setter Property="FontWeight" Value="Bold" />
